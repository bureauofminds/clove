package com.cloveHello.plugin
{
	import com.spice.recycle.events.DisposableEventDispatcher;
	
	public class CloveHelloPluginSettings extends DisposableEventDispatcher
	{
		[Bindable] 
		[Setting]
		public var displayName:String = "Craig";
		
		[Bindable] 
		[Setting]
		public var userName:String = "Clove";
		
		[Bindable] 
		[Setting]
		public var installed:Boolean;
		
	}
}